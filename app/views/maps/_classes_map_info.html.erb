<%= render partial: "map_info_address", locals: { marker_info: marker_info } %>

<ul class="listing_info">
  <% marker_info.class_listings.each do |class_listing| %>
    <li>
      <span class="day">
        <%= "#{ class_listing.day }:" %>
      </span>
      <%= mapinfo_swingclass_link(class_listing.event) %>
      <%= swingclass_cancelledmsg(class_listing.event) %>
    </li>
  <% end %>
</ul>
